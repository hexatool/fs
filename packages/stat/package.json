{
	"name": "@hexatool/fs-stat",
	"version": "0.0.1-next.1",
	"description": "Modular fs library",
	"keywords": [
		"fs",
		"file",
		"file system",
		"promise"
	],
	"homepage": "https://github.com/hexatool/fs",
	"bugs": {
		"url": "https://github.com/hexatool/fs/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/hexatool/fs"
	},
	"author": "MarioMH8",
	"license": "MIT",
	"type": "module",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"types": "./dist/sync.d.ts",
			"default": "./dist/hexatool-fs-stat-sync.mjs"
		},
		"./async": {
			"types": "./dist/async.d.ts",
			"default": "./dist/hexatool-fs-stat-async.mjs"
		}
	},
	"scripts": {
		"build": "vite build",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"test": "vitest run --no-isolate",
		"test:coverage": "vitest run --no-isolate --coverage",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@hexatool/fs-file-system": "0.0.1-next.1"
	},
	"engines": {
		"node": ">=14.16.0"
	}
}
