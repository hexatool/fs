{
	"name": "@hexatool/fs-stat",
	"version": "0.0.0",
	"description": "Modular fs library based on graceful-fs",
	"keywords": [
		"fs",
		"file",
		"file system",
		"promise"
	],
	"homepage": "https://github.com/hexatool/fs",
	"bugs": {
		"url": "https://github.com/hexatool/fs/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/hexatool/fs"
	},
	"author": "MarioMH8",
	"license": "MIT",
	"type": "module",
	"files": [
		"dist"
	],
	"module": "./dist/hexatool-fs-stat-sync.mjs",
	"types": "./dist/types/src/sync.d.ts",
	"exports": {
		".": {
			"types": "./dist/types/src/sync.d.ts",
			"default": "./dist/hexatool-fs-stat-sync.mjs"
		},
		"./async": {
			"types": "./dist/types/src/async.d.ts",
			"default": "./dist/hexatool-fs-stat-async.mjs"
		}
	},
	"scripts": {
		"build": "vite build",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"test": "vitest run --no-isolate",
		"test:coverage": "vitest run --no-isolate --coverage",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"graceful-fs": "4.2.10"
	},
	"devDependencies": {
		"@hexatool/eslint-config-typescript": "1.0.0",
		"@hexatool/tsconfig": "18.0.0",
		"@types/graceful-fs": "4.1.5",
		"@types/node": "18.11.9",
		"@vitest/coverage-c8": "0.24.5",
		"eslint": "8.26.0",
		"rollup-plugin-typescript2": "0.34.1",
		"typescript": "4.8.4",
		"vite": "3.2.2",
		"vitest": "0.24.5"
	},
	"engines": {
		"node": ">=12"
	}
}
