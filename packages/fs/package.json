{
	"name": "@hexatool/fs",
	"version": "0.0.1-next.7",
	"description": "Modular fs library",
	"keywords": [
		"fs",
		"file",
		"file system",
		"promise"
	],
	"homepage": "https://github.com/hexatool/fs",
	"bugs": {
		"url": "https://github.com/hexatool/fs/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/hexatool/fs"
	},
	"license": "MIT",
	"type": "module",
	"files": [
		"dist"
	],
	"module": "./dist/hexatool-fs-sync.mjs",
	"types": "./dist/sync.d.ts",
	"exports": {
		".": {
			"types": "./dist/sync.d.ts",
			"default": "./dist/hexatool-fs-sync.mjs"
		},
		"./async": {
			"types": "./dist/async.d.ts",
			"default": "./dist/hexatool-fs-async.mjs"
		}
	},
	"typesVersions": {
		"*": {
			"async": [
				"./dist/async.d.ts"
			]
		}
	},
	"scripts": {
		"build": "vite build",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"test": "vitest run --no-isolate",
		"test:coverage": "vitest run --no-isolate --coverage",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@hexatool/fs-copy": "0.0.1-next.4",
		"@hexatool/fs-crawl": "0.0.1-next.1",
		"@hexatool/fs-create-file": "0.0.1-next.4",
		"@hexatool/fs-create-link": "0.0.1-next.4",
		"@hexatool/fs-empty-dir": "0.0.1-next.4",
		"@hexatool/fs-make-dir": "0.0.1-next.4",
		"@hexatool/fs-move": "0.0.1-next.4",
		"@hexatool/fs-path-exists": "0.0.1-next.4",
		"@hexatool/fs-remove": "0.0.1-next.4",
		"@hexatool/fs-stat-util": "0.0.1-next.4",
		"@hexatool/fs-temporary": "0.0.1-next.6"
	},
	"engines": {
		"node": ">=14.16.0"
	}
}
