{
	"name": "@hexatool/fs-path-exists",
	"version": "0.0.0",
	"description": "Modular fs library",
	"keywords": [
		"fs",
		"file",
		"file system",
		"promise"
	],
	"homepage": "https://github.com/hexatool/fs",
	"bugs": {
		"url": "https://github.com/hexatool/fs/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/hexatool/fs"
	},
	"author": "MarioMH8",
	"license": "MIT",
	"type": "module",
	"files": [
		"dist"
	],
	"module": "./dist/hexatool-fs-path-exists-sync.mjs",
	"types": "./dist/types/src/sync.d.ts",
	"exports": {
		".": {
			"types": "./dist/types/src/sync.d.ts",
			"default": "./dist/hexatool-fs-path-exists-sync.mjs"
		},
		"./async": {
			"types": "./dist/types/src/async.d.ts",
			"default": "./dist/hexatool-fs-path-exists-async.mjs"
		}
	},
	"scripts": {
		"build": "vite build",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"test": "vitest run --no-isolate",
		"test:coverage": "vitest run --no-isolate --coverage",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@hexatool/fs-file-system": "0.0.0"
	},
	"devDependencies": {
		"@hexatool/eslint-config-typescript": "1.0.1",
		"@hexatool/tsconfig": "18.0.0",
		"@types/node": "18.11.17",
		"@vitest/coverage-c8": "0.25.3",
		"eslint": "8.30.0",
		"rollup-plugin-typescript2": "0.34.1",
		"typescript": "4.9.3",
		"vite": "3.2.5",
		"vitest": "0.25.3"
	},
	"engines": {
		"node": ">=14.16.0"
	}
}
